{
    "extends": "./tsconfig.paths.json",
    "compilerOptions": {
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es2016",
        "allowJs": false,
        "lib": [
            "es2015",
            "DOM"
        ],
        "types": [
			"@fighter/typings",
            "@citizenfx/server"
		],
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noEmit": true
    }
}
