{
	"extends": "packages/tsconfig/tsconfig.base.paths.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@app/*": [
				"./app/*"
			],
			"@helpers/*": [
				"./helpers/*"
			],
			"@components/*": [
				"./components/*"
			],
			"@entities/*": [
				"./entities/*"
			],
			"@infra/*": [
				"./infra/*"
			],
			"@providers/*": [
				"./providers/*"
			],
			"@useCases/*": [
				"./useCases/*"
			],
			"@models/*": [
				"./orm/models/*"
			],
			"@shared/*": [
				"../../../shared/*"
			]
		}
	}
}
